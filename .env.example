# Копируйте этот файл как .env и заполните значениями
# НЕ ДЕЛИТЕСЬ .env ФАЙЛОМ С ДАННЫМИ!

# Токен вашего бота от @BotFather
BOT_TOKEN=ваш_токен_бота_здесь

# ID вашего чата в Telegram (куда будут приходить заявки)
ADMIN_CHAT_ID=ваш_чат_id_здесь

# ID Google таблицы (из URL: https://docs.google.com/spreadsheets/d/ЭТО_ID/edit)
SPREADSHEET_ID=ваш_id_таблицы_здесь

# JSON ключ сервисного аккаунта Google (весь JSON в одну строку)
GOOGLE_CREDS_JSON={"type": "service_account", "project_id": "...", "private_key_id": "...", "private_key": "...", "client_email": "...", "client_id": "...", "auth_uri": "...", "token_uri": "...", "auth_provider_x509_cert_url": "...", "client_x509_cert_url": "...", "universe_domain": "..."}
